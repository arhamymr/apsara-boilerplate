{
  "id": "dashboard",
  "version": "1.0.0",
  "name": "Dashboard",
  "description": "Stats overview, activity feed, charts, and sidebar navigation",
  "apps": ["web"],
  "features": ["stats", "activity", "charts", "sidebar"],
  "requires": [],
  "optionalFor": [],
  "config": {
    "web": {
      "dependencies": ["recharts", "lucide-react", "date-fns"],
      "devDependencies": []
    }
  },
  "exposes": {
    "web": {
      "components": [
        "DashboardLayout",
        "StatsCard",
        "ActivityFeed",
        "AnalyticsChart",
        "Sidebar"
      ],
      "hooks": ["useStats", "useActivity", "useAnalytics"],
      "types": ["StatItem", "ActivityItem", "ChartData"]
    }
  }
}
